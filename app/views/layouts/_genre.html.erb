<table class = "table">
  <thead class="thead-light">
    <tr>
      <th>ジャンル検索</th>
    </tr>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
    <tr>
      <td>
        <%= link_to genre.name,  public_items_path(genre_id: genre.id), method: :get, class: "text-decoration-none text-dark" %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
